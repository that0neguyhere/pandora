function newTheme(ID,timepos,pitch,vol)
local sickz = sick
--sickz:Stop()
sickz.Volume = vol
--sickz.TimePosition = timepos
sickz.PlaybackSpeed = pitch
sickz.Pitch = pitch
sickz.SoundId = ID
sickz.Name = "wrecked"
sickz.Looped = true
currentThemePlaying = sickz.SoundId
currentVol = sickz.Volume
currentPitch = sickz.Pitch
--sickz:Play()
--coroutine.resume(coroutine.create(function()
--wait(0.05)
--end))
end


function newThemeCust(ID,timepos,pitch,vol)
local sickz = sick
sickz:Stop()
sickz.Volume = vol
sickz.TimePosition = timepos
sickz.PlaybackSpeed = pitch
sickz.Pitch = pitch
sickz.SoundId = ID
sickz.Name = "wrecked"
sickz.Looped = true
currentThemePlaying = sickz.SoundId
currentVol = sickz.Volume
currentPitch = sickz.Pitch
sickz:Play()
coroutine.resume(coroutine.create(function()
wait(0.05)
end))
end
--//=================================\\
