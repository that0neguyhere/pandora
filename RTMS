	CharFixerButton.TextStrokeColor3 = Color3.fromRGB(0,0+75*sick.PlaybackLoudness/200,0+75*sick.PlaybackLoudness/200)
	CharFixerButton.TextStrokeTransparency = 0
	CharFixerButton.BackgroundColor3 = Color3.new(.5,.5,.5)
	CharFixerButton.Text = "Character Fixer"
	CharFixerButton.Size = UDim2.new(.25,0,0,36)
	CharFixerButton.AnchorPoint = Vector2.new(.5,1)
	CharFixerButton.Position = UDim2.new(.4,0,0,0)
	gui.Parent = Player:FindFirstChildOfClass("PlayerGui")
	CharFixerButton.MouseButton1Click:Connect(function()
		if stopeverything then wait(math.huge) end
		Character:Destroy()
	end)
	local function fixgui()
		fixinggui = true
		local oldcolor = CharFixerButton.TextColor3
		pcall(function()
			gui:Destroy()
		end)
		gui = Instance.new("ScreenGui")
		gui.Name = randomstring()
		CharFixerButton = Instance.new("TextButton",gui)
		CharFixerButton.TextScaled = true
		CharFixerButton.Font = "Code"
		CharFixerButton.Name = randomstring()
		CharFixerButton.BorderSizePixel = 0
		CharFixerButton.BackgroundTransparency = .5
		CharFixerButton.TextStrokeColor3 = Color3.fromRGB(0,0+75*sick.PlaybackLoudness/200,0+75*sick.PlaybackLoudness/200)
		CharFixerButton.TextStrokeTransparency = 0
		CharFixerButton.TextColor3 = oldcolor
		CharFixerButton.BackgroundColor3 = Color3.new(.5,.5,.5)
		CharFixerButton.Text = "Character Fixer"
		CharFixerButton.Size = UDim2.new(.25,0,0,36)
		CharFixerButton.AnchorPoint = Vector2.new(.5,1)
		CharFixerButton.Position = UDim2.new(.4,0,0,0)
		gui.Parent = Player:FindFirstChildOfClass("PlayerGui")
		CharFixerButton.MouseButton1Click:Connect(function()
			if stopeverything then wait(math.huge) end
			Character:Destroy()
		end)
		fixinggui = false
	end
	Player:FindFirstChildOfClass("PlayerGui").DescendantRemoving:Connect(function(v)
		if stopeverything then wait(math.huge) end
		if not fixinggui then
			if v == gui or v:IsDescendantOf(gui) then
				fixgui()
			end
		end
	end)
	coroutine.resume(coroutine.create(function()
		local noob = 0
		repeat if stopeverything then wait(math.huge) end noob = noob + game:GetService("RunService").RenderStepped:Wait() until noob >= .1
		while true do
			if not fixinggui and not alreadyfixing then
				CharFixerButton.TextColor3 = Color3.fromRGB(0,0+75*sick.PlaybackLoudness/200,0+75*sick.PlaybackLoudness/200)
			end
			swait()
		end
	end))
end




