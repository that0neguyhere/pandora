local function characterfixer()
	if OffDeqas then
		alreadyfixing = true

		local camerapos = workspace.CurrentCamera.CFrame
		pcall(function()
			Character:Destroy()
		end)

		Character = game.Players.LocalPlayer.Character

		--Character.Name = randomstring()

		--if ViewFrameRem.Value == 0 then
		--		Character.Parent = workspace
		--elseif ViewFrameRem.Value == 1 then
		--Character.Parent = workspace
		--	end
		--Character.Parent = workspace
		Humanoid = Character.Humanoid
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
		if lplr.Name == USERNAME then
			workspace.CurrentCamera.CameraSubject = Humanoid
			workspace.CurrentCamera.CameraType = "Scriptable"
			workspace.CurrentCamera.CFrame = camerapos
			coroutine.resume(coroutine.create(function()
				swait(5)
				workspace.CurrentCamera.CameraType = "Custom"
			end))
		end
		LeftArm = Character["Left Arm"]
		RightArm = Character["Right Arm"]
		LeftLeg = Character["Left Leg"]
		RightLeg = Character["Right Leg"]
		lerpintensity = 0.4
		Head = Character.Head
		Torso = Character.Torso
		RootPart = Character.HumanoidRootPart
		if lplr.Name == USERNAME then
			RootPart.CFrame = charpos.Value
			Player.Character = nil
		end
		Hole3 = RightArm
		ThiccHole = RightArm
		Hole = RightArm
		Holetwo = Character["Right Arm"]
		RightArm = RightArm
		root = Character.HumanoidRootPart
		Character:FindFirstChildOfClass("ForceField"):Destroy()


		Humanoid.DisplayDistanceType = "None"

		Glitcher1 = script.Glitcher:Clone()
		Glitcher1.Parent = RightLeg
		Glitcher1.NeonPart3.Weld.Part1 = RightLeg
		Glitcher1.NeonPart3.Weld.C0 = CF(0,0.3,0)


		Glitcher = script.Glitcher2:Clone()
		Glitcher.Parent = LeftArm
		Glitcher.NeonPart3.Weld.Part1 = LeftArm
		Glitcher.NeonPart3.Weld.C0 = CF(0,0.3,0)

		--weaponpapkap = script.Uhweapons:Clone()
		--weaponpapkap.Parent = Character

		CreationFolder = IT("Folder",Character)
		CreationFolder.Name = "CreationFolder"




		RootJoint = RootPart.RootJoint
		Neck = Torso["Neck"]
		RightShoulder = Torso["Right Shoulder"]
		LeftShoulder = Torso["Left Shoulder"]
		RightHip = Torso["Right Hip"]
		LeftHip = Torso["Left Hip"]
		Effects = IT("Folder", Character)
		Effects.Name = "Effects"
		PlayerSize = 1
