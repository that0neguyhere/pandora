				LASTPART = Horn
				Horn.Color = C3((i/15),0,0)
				local MSH = IT("BlockMesh",Horn)
				MSH.Scale = VT(0.16*MATH,1,0.16*MATH)
			end
		end
		local LASTPART = Head
		for i = 1, 15 do
			local MATH = (1-(i/16))
			if LASTPART == Head then
				local Horn = CreatePart(3, CreationFolder, "Neon", 0, 0, "Dirt brown", "Horn", VT(1,0.25,1),false)
