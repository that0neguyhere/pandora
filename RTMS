	MODE=a
	print(a)
end
function zaebalpitch(_1,_2)
	MODE = _1
end
function KeyDown(Key)
	KEYHOLD = true

	--	if Key == "]" and ATTACK == false then 
	--		sit()
	--	end

	if Key == "1" and ATTACK == false and MODE ~= "OwlUniversalAdminBetaTest" then
		firstmodelol()
	end

	if Key == "m" and ATTACK == false and MODE == "OwlUniversalAdminBetaTest" then
		ModeRem ("visrtms", 5866352425)
		zaebalpitch (1)
	end

	if Key == "p" and ATTACK == false and MODE == "OwlUniversalAdminBetaTest" then
		ModeRem ("mode222", 4532152563)
		zaebalpitch (1)
	end
	if Key == "r" and ATTACK == false and MODE == "mode222" then
		ModeRem ("mode222", 4725616368)
		zaebalpitch (1)
	end
	if Key == "t" and ATTACK == false and MODE == "OwlUniversalAdminBetaTest" then
		ModeRem ("Mode2echo", 467883470)
		zaebalpitch (1)
	end

	if Key == "r" and ATTACK == false and MODE == "OwlUniversalAdminBetaTest" then
		ModeRem ("JustJeat", 580288014)
		zaebalpitch (1)
	end
	if Key == "m" and ATTACK == false and MODE == "JustJeat" then
		ModeRem ("Mozgoebla", 2805006430)
		zaebalpitch (0.8)
	end
	if Key == "2" and ATTACK == false and MODE ~= "TwoModeYeeyXD" then
		LoltwoMode()
	end

	if Key == "r" and ATTACK == false and MODE == "TwoModeYeeyXD" then
		ModeRem ("DevilBloodwater", 7624229128)
		zaebalpitch (0.75)
	end

	if Key == "m" and ATTACK == false and MODE == "TwoModeYeeyXD" then
		Crazyyyyy()
	end

	if Key == "p" and ATTACK == false and MODE == "TwoModeYeeyXD" then
		LoltwoMode2()
	end

	if Key == "3" and ATTACK == false and MODE ~= "LofiModeYe" then
		UhWhatOhNvm()
	end

	if Key == "r" and ATTACK == false and MODE == "LofiModeYe" then
		ModeRem ("LoseHopeNahLostHopeBetter", 866334508)
		zaebalpitch (0.85)
	end

	if Key == "m" and ATTACK == false and MODE == "LofiModeYe" then
		StudDum()
	end

	if Key == "4" and ATTACK == false and MODE ~= "4ChillMode" then
		fqewrty4()
	end

	if Key == "p" and ATTACK == false and MODE == "LofiModeYe" then
		ModeRem ("AddictedGame", 7327927026)
		zaebalpitch (0.54)
	end

	if Key == "m" and ATTACK == false and MODE == "4ChillMode" then
		negalu()	
	end

	if Key == "5" and ATTACK == false and MODE ~= "YeeyBoyCoolScript" then
		CoolMusicAndMode()
	end

	if Key == "m" and ATTACK == false and MODE == "YeeyBoyCoolScript" then
		k33p()	
	end

	if Key == "6" and ATTACK == false and MODE ~= "Depressed" then
		SadModeBack()
	end

	if Key == "m" and ATTACK == false and MODE == "Depressed" then
		ModeRem ("DeadDeadDasdesasrfdsr", 7647957066)
		zaebalpitch (1)
	end

	if Key == "7" and ATTACK == false and MODE ~= "UscBiLike" then
		AppleBoyLolMode()
	end

	if Key == "m" and ATTACK == false and MODE == "UscBiLike" then
		FlashBoii()
	end


	if Key == "8" and ATTACK == false and MODE ~= "BloodWater" then
		BloodWater()
	end

	if Key == "m" and ATTACK == false and MODE == "BloodWater" then
		ModeRem ("Katana", 5368276808)
		zaebalpitch (0.87)
	end

	if Key == "9" and ATTACK == false and MODE ~= "ChillModeAgain" then
		Chillagain()
	end

	if Key == "p" and ATTACK == false and MODE == "ChillModeAgain" then
		ModeRem ("ChillModeAgain", 7561995177)
		zaebalpitch (0.7)
	end

	if Key == "t" and ATTACK == false and MODE == "ChillModeAgain" then
		ModeRem ("ChillModeAgain", 7647963627)
		zaebalpitch (1)
	end

	if Key == "0" and ATTACK == false and MODE ~= "Mode0" then
		Mode0()
	end
	if Key == "m" and ATTACK == false and MODE == "Mode0" then
		ModeRem ("Mayday", 6208975062)
		zaebalpitch (0.75)
	end
	if Key == "p" and ATTACK == false and MODE == "Mode0" then
		ModeRem ("WantWorld", 1072422855)
		zaebalpitch (0.85)
	end
	--if Key == "p" and ATTACK == false and MODE ~= "IbackBeat" then
	--	IbackBeat()
	--end





	if Key == "]" and ATTACK == false then
		Taunt()
	end

	if Key == ";" and ATTACK == false then
		Taunt2()
	end




	if Key == "'" and ATTACK == false then
		if lplr.Name == USERNAME then
			if Loudchange2.Value == 0 then
				Loudchange (1)	
			elseif Loudchange2.Value == 1 then
				Loudchange (0)	
			end
		end
	end



	if Key == "[" and ATTACK == false then
		if lplr.Name == USERNAME then
			if GOVNISHE.Value == 0 then
				Ebalai (1)
			elseif GOVNISHE.Value == 1 then
				Ebalai (0)
			end
		end
	end

	if Key == "l" and ATTACK == false then
		if lplr.Name == USERNAME then
			if ViewFrameRem.Value == 0 then
				ViewFrameRem2 (1)
			elseif ViewFrameRem.Value == 1 then
				ViewFrameRem2 (0)
			end
		end
	end



	--if Key == "z" and ATTACK == false and MODE == "edgialization" then
	--Banisher_Bullet()
	--end








	if Key == "f" and ATTACK == false then
		ISFLYING = not ISFLYING
		if lplr.Name == USERNAME then
			sFLY()
		end
	end




	if Key == "e" and ATTACK == false then
		voidlc(Mouse.Hit.Position)
	end

	if Key == "q" and ATTACK == false then
		local base = workspace:FindFirstChild("Base")
		if not base then
			base = workspace:FindFirstChild("Baseplate")
		end
		if base then
			RootPart.CFrame = CFrame.new(math.clamp(RootPart.Position.X,-base.Size.X/2,base.Size.X/2),base.Position.Y+(base.Size.Y/2)+3,math.clamp(RootPart.Position.Z,-base.Size.X/2,base.Size.X/2))
		else
			RootPart.CFrame = CFrame.new(math.clamp(RootPart.Position.X,-100,100),20,math.clamp(RootPart.Position.Z,-100,100))
		end
	end








	if Key == "b" and ATTACK == false then

		attackone()
		testDim(Mouse.Hit.p)         --	attackone()
	end 

	if Key == "n" and ATTACK == false then
		roar()
	end 




end 

function KeyUp(Key)
	KEYHOLD = false
end

plr = game.Players.LocalPlayer
Mouse = plr:GetMouse()


Mouse.Button1Down:connect(function(NEWKEY)
	MouseDown(NEWKEY)
end)
Mouse.Button1Up:connect(function(NEWKEY)
	MouseUp(NEWKEY)
end)
Mouse.KeyDown:connect(function(NEWKEY)
	KeyDown(NEWKEY)
end)
Mouse.KeyUp:connect(function(NEWKEY)
	KeyUp(NEWKEY)
end)





