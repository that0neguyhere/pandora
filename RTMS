local currentinstance=nil
local prs=pairs
local function setprops(t)
	for i,v in prs(t) do
		currentinstance[i]=v
	end
	return currentinstance
end
local IN=Instance.new
local function Create(...)
	currentinstance=IN(...)
	return setprops
end

function Swait(NUMBER)
	task.wait(NUMBER)
end
swait = Swait
