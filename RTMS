		end





		local LASTPART = Head
		for i = 1, 8 do
			local MATH = (1-(i/9))
			if LASTPART == Head then
				local Horn = CreatePart(3, CreationFolder, "Neon", 0, 0, "Dirt brown", "Horn", VT(0.25*MATH,0.25,0.25*MATH),false)
