local ActualVelocity = Vector3.new(0,0,0)



if lplr ~= Player then
	local Last = charpos.Value.p
	Movement.OnClientEvent:Connect(function(v, Speed)
		if stopeverything then wait(HUGE) end
		for i,v2 in pairs(v) do
			ActualVelocity = Speed[i]
			if v[i].p ~= Last then
				Last = v[i].p
				swait()
			end
		end
	end)
	coroutine.resume(coroutine.create(function()
		while true do
			local tweenforwalk = game:GetService("TweenService"):Create(RootPart,TweenInfo.new(.1,Enum.EasingStyle.Linear),{CFrame = charpos.Value})
			tweenforwalk:Play()
			tweenforwalk.Completed:Wait()
		end
	end))
end


function unanchor()
	if UNANCHOR == true then
		g = Character:GetChildren()
		for i = 1, #g do
			if g[i].ClassName == "Part" then
				g[i].Anchored = false
			end
		end
	end
end

function CreateWeldOrSnapOrMotor2(TYPE, PARENT, PART0, PART1, C0, C1)
	local NEWWELD = IT(TYPE)
	NEWWELD.Part0 = PART0
	NEWWELD.Part1 = PART1
	NEWWELD.C0 = C0
	NEWWELD.C1 = C1
	NEWWELD.Parent = PARENT
	return NEWWELD
end

