	RWP1W.C0 = RWP1W.C0:lerp(CF.N(-0,0+6*M.C(WingSine/32),-1)*CF.A(M.R(100+-200*M.C(WingSine/50)),0,M.R(240+1000*M.C(WingSine/400))),.2)
