	LWP2W.C0 = LWP2W.C0:lerp(CF.N(3.5+3.5*M.C(WingSine/100),3+2*M.C(WingSine/100),-.9)*CF.A(M.R(120+6000*M.C(WingSine/400)),0+400*M.C(WingSine/3000),M.R(-90+400*M.C(WingSine/300))),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(3.5+3.5*M.C(WingSine/100),3+2*M.C(WingSine/100),-.9)*CF.A(M.R(240+6000*M.C(WingSine/400)),0+400*M.C(WingSine/3000),M.R(-90+400*M.C(WingSine/300))),.2)
	RWP1W.C0 = RWP1W.C0:lerp(CF.N(-3.5+-3.5*M.C(WingSine/100),3+2*M.C(WingSine/100),-.9)*CF.A(M.R(0+6000*M.C(WingSine/400)),0+400*M.C(WingSine/3000),M.R(-90+400*M.C(WingSine/300))),.2)
	RWP2W.C0 = RWP2W.C0:lerp(CF.N(-3.5+-3.5*M.C(WingSine/100),3+2*M.C(WingSine/100),-.9)*CF.A(M.R(120+6000*M.C(WingSine/400)),0+400*M.C(WingSine/3000),M.R(-90+400*M.C(WingSine/300))),.2)
	RWP3W.C0 = RWP3W.C0:lerp(CF.N(-3.5+-3.5*M.C(WingSine/100),3+2*M.C(WingSine/100),-.9)*CF.A(M.R(240+6000*M.C(WingSine/400)),0+400*M.C(WingSine/3000),M.R(-90+400*M.C(WingSine/300))),.2)
	--SPIN
	RWP4W.C0 = RWP4W.C0:lerp(CF.N(0*M.C(WingSine/62),-1.64+0*M.S(WingSine/42),-1.2)*CF.A(0,0,M.R(-0-950*M.C(WingSine/400))),.2)
