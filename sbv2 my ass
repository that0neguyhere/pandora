					end		
				end
				VALUE1 = false
				FT.Parent = nil
				FRA.Parent = nil
				FLA.Parent = nil
				FRL.Parent = nil
				FLL.Parent = nil
				for _,v in next, Character:GetDescendants() do
					if(v:IsA'DataModelMesh')then
