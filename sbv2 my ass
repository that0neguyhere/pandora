				VALUE1 = true
				wait(2)
				CreateSound(749189256,RootPart,2,math.random(5, 15) / 10,false)
				for i=1,25 do
					Swait()
					FT.Parent = Torso
					FRA.Parent = RightArm
					FLA.Parent = LeftArm
					FRL.Parent = RightLeg
					FLL.Parent = LeftLeg
